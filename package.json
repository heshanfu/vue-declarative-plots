{
  "name": "vue-declarative-plots",
  "version": "1.0.0",
  "repository": "https://github.com/keller-mark/vue-declarative-plots",
  "author": "Mark Keller",
  "license": "MIT",
  "private": false,
  "main": "dist/vue-declarative-plots.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.js --open --inline --hot",
    "build:debug": "cross-env NODE_ENV=debug webpack --config build/webpack.dist.js",
    "build:release": "cross-env NODE_ENV=production webpack --config build/webpack.dist.js",
    "build:examples": "webpack --config build/webpack.examples.js",
    "build": "yarn run build:debug && yarn run build:release && yarn run build:examples"
  },
  "dependencies": {
    "d3": "^5.7.0",
    "lodash": "^4.17.11",
    "save-svg-as-png": "^1.4.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "deep-assign": "^2.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "vue": "^2.5.13",
    "vue-loader": "^11.3.3",
    "vue-style-loader": "^3.0.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-server": "^2.7.1"
  }
